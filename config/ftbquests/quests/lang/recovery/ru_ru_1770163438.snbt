	"chapters/industrial_foregoing.snbt": {
		quest.4DB5DD6E20619B4D.quest_desc: {
			old: ["Used to make machines better."]
			new: ["Used to make machines better. There's 8 tiers of addons, and 4 more specifically for Range."]
		}
		quest.0EC2053B191C55C6.quest_desc: {
			old: ["Extracts latex from logs, runs faster when given power.\\n\\nCheck JEI for acceptable logs and Latex amounts. The best log to use is Acacia."]
			new: ["Extracts Latex from logs, runs faster when given power.\\n\\nCheck JEI for acceptable logs and Latex amounts. The best log to use is Acacia or Mangrove."]
		}
		quest.2CCFEE98FE3B2E97.quest_desc: {
			old: ["These machines are used to automate block placing/breaking. This is especially useful when automating latex."]
			new: ["These machines are used to automate block placing/breaking. This is especially useful when automating Latex."]
		}
		quest.1823CC81D613892B.quest_desc: {
			old: [
				"Passive Mobs -> More Pink Slime"
				"Hostile Mobs -> More Meat"
			]
			new: ["The Mob Slaughter Factory &ldeletes&r mobs in its working area, and converts that mob into &#FF69B4Pink Slime&r and &6Liquid Meat&r.\\n\\nPassive Mobs -> More &#FF69B4Pink Slime&r\\nHostile Mobs -> More &6Meat&r\\n\\nThis machine supports Range Addons."]
		}
		quest.540B857F043C24D5.quest_desc: {
			old: ["The Bioreactor uses Biofuel made from the Biofuel Generator to generate FE."]
			new: ["The Bioreactor produces &5Biofuel&r with &9Water&r and a bunch of different materials (check JEI for all valid Bioreactor Inputs). The more types of materials you provide, the more efficient the Bioreactor is at producing &5Biofuel&r.\\n\\nThe Biofuel Generator uses &5Biofuel&r made from the &5Bioreactor&r to generate FE."]
		}
		quest.3E6706BC4C318A40.quest_desc: {
			old: ["The Sewer is used to make Sewage, which is used to make Fertilizer."]
			new: ["The Sewer makes &#8B4513Sewage&r from animals in its working area, as well as absorb &aExperience&r from players to convert into &aEssence&r.\\n\\nThe Sewage Composter can turn &#8B4513Sewage&r into &#8B4513Fertilizer&r.\\n\\nThe Sewer supports Range Addons."]
		}
		quest.3CB7884B7B32CF00.quest_desc: {
			old: [
				"Getting your first &bEther Gas&r might be a little bit scary.\\n\\n&bEther Gas&r is made from drilling a Wither, using a Fluid Drill with a &5Purple Lens&r."
				""
				"But don't worry about the Wither, Industrial Foregoing has a machine just to help you with that task. The &4Stasis Chamber&r is a machine that freezes in place anything that is on top of it in a 3x3 area, including the Wither."
				""
				"&cHope you don't run out of power, because if you do... I hope you're ready to fight the &0Wither&c."
			]
			new: ["Getting your first &bEther Gas&r might be a little bit scary.\\n\\n&bEther Gas&r is made from drilling a Wither, using a Fluid Drill with a &5Purple Lens&r.\\n\\nBut don't worry about the Wither, Industrial Foregoing has a machine just to help you with that task. The &4Stasis Chamber&r is a machine that freezes in place anything that is on top of it in a 3x3 area, including the Wither.\\n\\n&cHope you don't run out of power, because if you do... I hope you're ready to fight the &0Wither&c. Because of this, I recommend using some kind of Wither-Proof Glass, such as Tinted Glass from Mob Grinding Utils, or Crystalix Glass with Reinforced Mode enabled.&c"]
		}
		quest.6FF04DD735346BED.quest_desc: {
			old: ["Turns Latex into Dry Rubber."]
			new: ["Turns Latex into Dry Rubber. Uses 750 mB of Latex and 500 mB of &9Water&r to produce 1 Dry Rubber."]
		}
		quest.418E57E34FFC19E1.quest_desc: {
			old: ["Suggestion: Use some sort of Wither-Proof Glass."]
			new: ["The Ore Laser Base is &bIndustrial Foregoing&r's Void Miner, being able to conjure ores out of thin air. Lens allow you to favor a specific color of ore, such as Blue for Lapis.\\n\\nThe Fluid Laser Base produces Fluids out of either thin air or from specific mobs. These require you to use specific Lens. Mobs that are used to produce Fluids will take damage, so using a Stasis Chamber for it is recommended.\\n\\nLaser Drills provide progress to the Laser Base in its working area."]
		}
		quest.1BA3D15FFE7DBE59.quest_desc: {
			old: ["In &bIndustrial Foregoing&r, one of the main resources is &fLatex.&r Its used to craft Machine Frames, needed to make machines and upgrades.\\n\\nMaking Latex is pretty simple, &aFluid Extractor&r extracts Latex from &6Logs&r (Acacia gives the most).\\n\\n------------------------\\n\\nNow about making Plastic: Plastic results from smelting Dry Rubber - which is made in the &aLatex Processing Unit&r, that transforms Latex into Dry Rubber.\\n\\n&bSo basically Latex -> Dry Rubber -> Plastic."]
			new: ["In &bIndustrial Foregoing&r, one of the main resources is &fLatex.&r Its used to craft Machine Frames, needed to make machines and upgrades.\\n\\nMaking Latex is pretty simple, &aFluid Extractor&r extracts Latex from &6Logs&r (Acacia and Mangrove give the most).\\n\\n------------------------\\n\\nNow about making Plastic: Plastic results from smelting Dry Rubber - which is made in the &aLatex Processing Unit&r, that transforms Latex into Dry Rubber.\\n\\n&bSo basically Latex -> Dry Rubber -> Plastic."]
		}
		quest.0E8647B8EB4AAC41.quest_desc: {
			old: ["Passive Mobs -> More Pink Slime.\\nHostile Mobs -> More Meat."]
			new: ["Passive Mobs -> More &#FF69B4Pink Slime&r\\nHostile Mobs -> More &6Meat&r"]
		}
	}
